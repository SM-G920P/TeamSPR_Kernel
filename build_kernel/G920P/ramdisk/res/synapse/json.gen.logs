#!/sbin/busybox sh

cat << CTAG
{
    name:LOGS,
    elements:[
	{ SPane:{
		title:"Android Logger",
		description:" Save the logcat (app/system debug output) to file."
	}},
	{ SSpacer:{
		height:1
	}},
	{ SCheckBox:{
		description:" You must Enable to save the latest logcat. Default is Enabled.",
		label:"Enable Android logger",
		default:`cat /sys/kernel/logger_mode/logger_mode`,
		action:"generic /sys/kernel/logger_mode/logger_mode"
	}},
	{ SSpacer:{
		height:1
	}},
	{ SButton:{
		label:"Save logcat",
		action:"log logcat"
	}},
	{ SSpacer:{
		height:1
	}},
	{ SPane:{
		title:"Kernel Logger",
		description:" Save the dmesg (active kernel output) or last_ksmg (same as dmesg but from the last shutdown)."
	}},
	{ SSpacer:{
		height:1
	}},
	{ SCheckBox:{
		description:" You must Enable to save the latest dmesg. Default is Enabled.",
		label:"Enable printk",
		default:`cat /sys/kernel/printk_mode/printk_mode`,
		action:"generic /sys/kernel/printk_mode/printk_mode"
	}},
	{ SSpacer:{
		height:1
	}},
	{ SButton:{
		label:"Save dmesg",
		action:"log dmesg"
	}},
	{ SSpacer:{
		height:1
	}},
	{ SButton:{
		label:"Save last_kmsg",
		action:"log kmsg"
	}},
	{ SSpacer:{
		height:1
	}},
    ]
